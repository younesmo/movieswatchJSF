<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">
  	
	<navigation-rule> 
	   		<from-view-id>/*</from-view-id>  
		<navigation-case> 
    		 <from-outcome>registration</from-outcome> 
    	 	<to-view-id>/registration.xhtml</to-view-id> 
    	 	<redirect/>
 	  	</navigation-case>
 	  	<navigation-case> 
    		 <from-outcome>welcome</from-outcome> 
    	 	<to-view-id>/restricted/welcome.xhtml</to-view-id> 
    	 	<redirect/>
 	  	</navigation-case>
 	  	<navigation-case> 
    		 <from-outcome>profil</from-outcome> 
    	 	<to-view-id>/restricted/profile.xhtml</to-view-id> 
    	 	<redirect/>
 	  	</navigation-case>
 	  	<navigation-case> 
    		 <from-outcome>movies</from-outcome> 
    	 	<to-view-id>/restricted/movies.xhtml</to-view-id> 
    	 	<redirect/>
 	  	</navigation-case>
 	  	<navigation-case> 
    		 <from-outcome>cart</from-outcome> 
    	 	<to-view-id>/restricted/cart.xhtml</to-view-id> 
    	 	<redirect/>
 	  	</navigation-case>
 	  	<navigation-case> 
    		 <from-outcome>login</from-outcome> 
    	 	<to-view-id>/login.xhtml</to-view-id>
    	 	<redirect/> 
 	  	</navigation-case>
 	  	<navigation-case> 
    		 <from-outcome>orders</from-outcome> 
    	 	<to-view-id>/restricted/orders.xhtml</to-view-id>
    	 	<redirect/> 
 	  	</navigation-case>
 	  	<navigation-case> 
    		 <from-outcome>allOrders</from-outcome> 
    	 	<to-view-id>/accounting/allOrders.xhtml</to-view-id> 
    	 	<redirect/>
 	  	</navigation-case>
 	  	<navigation-case> 
    		 <from-outcome>members</from-outcome> 
    	 	<to-view-id>/admin/members.xhtml</to-view-id> 
    	 	<redirect/>
 	  	</navigation-case>
	</navigation-rule>
	
	<navigation-rule> 
   		<from-view-id>/restricted/movies.xhtml</from-view-id>  
 	  	<navigation-case> 
    		 <from-outcome>movieDetails</from-outcome> 
    	 	<to-view-id>/restricted/movie.xhtml</to-view-id> 
    	 	<redirect/>
 	  	</navigation-case>
	</navigation-rule>
	
	<navigation-rule> 
   		<from-view-id>/restricted/cart.xhtml</from-view-id>  
 	  	<navigation-case> 
    		 <from-outcome>orders</from-outcome> 
    	 	<to-view-id>/restricted/orders.xhtml</to-view-id> 
    	 	<redirect/>
 	  	</navigation-case>
	</navigation-rule>
	
	<navigation-rule> 
   		<from-view-id>/restricted/orders.xhtml</from-view-id>  
 	  	<navigation-case> 
    		 <from-outcome>order</from-outcome> 
    	 	<to-view-id>/restricted/order.xhtml</to-view-id> 
 	  	</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
	 <from-view-id>/login.xhtml</from-view-id>  
	<navigation-case> 
    		 <from-outcome>login</from-outcome> 
    	 	<to-view-id>/login.xhtml</to-view-id> 
 	  	</navigation-case>
 	  </navigation-rule>
 	  
 	 <navigation-rule>
		 <from-view-id>/restricted/profile.xhtml</from-view-id>  
		<navigation-case> 
    			 <from-outcome>updateProfil</from-outcome> 
    		 	<to-view-id>/restricted/updateProfil.xhtml</to-view-id>
 	 	</navigation-case>
		<navigation-case> 
    			 <from-outcome>editProfil</from-outcome> 
    		 	<to-view-id>/restricted/editProfil.xhtml</to-view-id>
 	 	</navigation-case>
 	 	<navigation-case> 
    			 <from-outcome>editPassword</from-outcome> 
    		 	<to-view-id>/restricted/editPassword.xhtml</to-view-id>
 	 	</navigation-case>
 	 	<navigation-case> 
    			 <from-outcome>addProfil</from-outcome> 
    		 	<to-view-id>/restricted/addProfil.xhtml</to-view-id> 
 	 	</navigation-case>
 	 </navigation-rule>
 	 
 	 <navigation-rule>
		 <from-view-id>/restricted/editProfil.xhtml</from-view-id>  
		<navigation-case> 
    			 <from-outcome>profil</from-outcome> 
    		 	<to-view-id>/restricted/profile.xhtml</to-view-id>
 	 	</navigation-case>
 	 </navigation-rule>
 	 
 	 <navigation-rule>
		 <from-view-id>/restricted/editPassword.xhtml</from-view-id>  
		<navigation-case> 
    			 <from-outcome>profil</from-outcome> 
    		 	<to-view-id>/restricted/profile.xhtml</to-view-id>
 	 	</navigation-case>
 	 </navigation-rule>
 	 
 	  	 <navigation-rule>
		 <from-view-id>/restricted/addProfil.xhtml</from-view-id>  
		<navigation-case> 
    			 <from-outcome>profil</from-outcome> 
    		 	<to-view-id>/restricted/profile.xhtml</to-view-id> 
    		 	  <redirect/>  		 	
 	 	</navigation-case> 	 	
 	 </navigation-rule>
 	 
 	 	<navigation-rule> 
   		<from-view-id>/admin/members.xhtml</from-view-id>  
 	  	<navigation-case> 
    		 <from-outcome>addMember</from-outcome> 
    	 	<to-view-id>/admin/addMember.xhtml</to-view-id> 
 	  	</navigation-case>
	</navigation-rule>
	
	<navigation-rule> 
   		<from-view-id>/admin/members.xhtml</from-view-id>  
 	  	<navigation-case> 
    		 <from-outcome>updateMember</from-outcome> 
    	 	<to-view-id>/admin/updateMember.xhtml</to-view-id> 
 	  	</navigation-case>
	</navigation-rule>
	
	 	 	<navigation-rule> 
   		<from-view-id>/admin/addMember.xhtml</from-view-id>  
 	  	<navigation-case> 
    		 <from-outcome>members</from-outcome> 
    	 	<to-view-id>/admin/members.xhtml</to-view-id> 
    	 	<redirect/>
 	  	</navigation-case>
	</navigation-rule>
</faces-config>
